<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.unej.ilkom.simulatour">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Sewa" />
        <activity android:name=".SimulatorDetail" />
        <activity android:name=".About" />
        <activity android:name=".Wisata" />
        <activity android:name=".Penginapan" />
        <activity android:name=".Transportasi" />
        <activity android:name=".Makanan" />
        <activity android:name=".WisataDetail" />
        <activity android:name=".MakananDetail" />
        <activity android:name=".SewaDetail" />
        <activity android:name=".TransportasiDetail" />
        <activity android:name=".PenginapanDetail"></activity>
    </application>

</manifest>